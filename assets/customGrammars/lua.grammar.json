                        {
                            "match": "\\b([a-zA-Z_$][\\w$]*)\\b",
                            "name": "variable.other.luau"
                        }
                    ]
                },
                {
                    "begin": "(?<=(=|\\bin\\b))",
                    "end": "(?=\\bdo\\b)",
                    "patterns": [
                        {
                            "match": ",",
                            "name": "punctuation.separator.comma.luau"
                        },
                        {
                            "include": "#simple_expression"
                        }
                    ]
                }
            ]
        },
        "repeat_loops": {
            "begin": "\\brepeat\\b",
            "beginCaptures": {
                "0": {
                    "name": "keyword.control.repeat.luau"
                }
            },
            "end": "\\buntil\\b",
            "endCaptures": {
                "0": {
                    "name": "keyword.control.until.luau"
                }
            },
            "patterns": [
