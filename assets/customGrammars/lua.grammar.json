{
    "information_for_contributors": [
        "This file is a custom grammar for luau created for the main project in this repository.",
        "It uses the base syntax of Lua 5.1, defined here: https://www.lua.org/manual/5.1/manual.html#2",
        "and it uses any additional syntax defined here: https://luau-lang.org/syntax"
    ],
    "version": "https://github.com/ambrosiogabe/MathAnimation/tree/master/assets/customGrammars/luau.grammar.json",
    "name": "Luau",
    "scopeName": "source.luau",
    "comment": "Luau Syntax: version 0.1",
    "patterns": [
        {
            "include": "#comments"
        },
        {
            "include": "#local_typed_var"
        },
        {
            "include": "#type_declaration"
        },
        {
            "include": "#while_loops"
        },
        {
            "include": "#for_loops"
        },
        {
            "include": "#if_else_stmts"
        },
        {
            "include": "#repeat_loops"
        },
        {
            "include": "#block"
        },
        {
            "include": "#keywords"
        },
        {
            "include": "#type_casts"
        },
        {
            "include": "#function_definitions"
        },
        {
            "include": "#function_calls"
        },
        {
            "include": "#stringLiterals"
        },
        {
            "include": "#numberLiterals"
        },
        {
            "include": "#table_constructor"
        },
        {
            "include": "#constants"
        },
        {
            "include": "#operators"
        },
        {
            "match": "\\bcontinue\\b",
            "name": "keyword.control.luau"
        },
        {
            "include": "#variable_names"
        }
    ],
    "repository": {
        "type_declaration": {
            "patterns": [
                {
                    "begin": "(?:\\b(export)\\b)?\\s*\\b(type)\\b\\s*([a-zA-Z_$][\\w$]*)\\b\\s*=",
                    "beginCaptures": {
                        "1": {
                            "name": "keyword.control.export.luau"
                        },
                        "2": {
                            "name": "storage.type.type.luau"
                        },
                        "3": {
                            "name": "entity.name.type.alias.luau"
                        }
                    },
                    "end": "(?m)((?<=([^\\-]>|\\w|\\}|'|\")(\\b|$|\\s))(?!\\s*(\\&|<|(\\)\\s*\\->)|\\|)))|((?<=\\))(\\b|$|\\s))(?!\\s*(\\->|\\&|\\|))",
                    "patterns": [
                        {
                            "include": "#type_annotations"
                        }
                    ]
                },
                {
                    "begin": "(?:\\b(export)\\b)?\\s*\\b(type)\\b\\s*([a-zA-Z_$][\\w$]*)\\b\\s*(?=<)",
                    "beginCaptures": {
                        "1": {
                            "name": "keyword.control.export.luau"
                        },
                        "2": {
                            "name": "storage.type.type.luau"
                        },
                        "3": {
                            "name": "entity.name.type.alias.luau"
                        }
                    },
                    "end": "(?m)((?<=([^\\-]>|\\w|\\}|'|\")(\\b|$|\\s))(?!\\s*(\\&|<|(\\)\\s*\\->|=)|\\|)))|((?<=\\))(\\b|$|\\s))(?!\\s*(\\->|\\&|\\|))",
                    "patterns": [
                        {
                            "begin": "<",
                            "end": ">\\s*=",
                            "patterns": [
                                {
                                    "match": "\\s*([a-zA-Z_$][\\w]*)\\s*(\\.\\.\\.)?\\s*",
                                    "captures": {
                                        "1": {
                                            "name": "entity.name.type.luau"
                                        }
                                    }
                                },
                                {
                                    "match": "\\s*,\\s*",
                                    "name": "punctuation.separator.comma.luau"
                                }
                            ]
                        },
                        {
                            "include": "#type_annotations"
                        }
                    ]
                }
            ]
        },
        "local_typed_var": {
            "begin": "\\b(local)\\b\\s*\\b([a-zA-Z_$][\\w$]*)\\b\\s*:",
            "beginCaptures": {
                "1": {
                    "name": "keyword.control.local.luau"
                },
                "2": {
                    "name": "variable.other.luau"
                }
            },
            "end": "(?m)((?<=([^\\-]>|\\w|\\}|'|\")(\\b|$|\\s))(?!\\s*(\\&|<|(\\)\\s*\\->)|\\|)))|((?<=\\))(\\b|$|\\s))(?!\\s*(\\->|\\&|\\|))",
            "patterns": [
                {
                    "include": "#type_annotations"
                }
            ]
        },
        "type_annotations": {
            "patterns": [
                // Table types
                {
                    "begin": "{",
                    "beginCaptures": {
                        "0": {
                            "name": "punctuation.bracket.curly.begin.luau"
                        }
                    },
                    "end": "}",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.bracket.curly.begin.luau"
                        }
                    },
                    "patterns": [
                        {
                            "begin": "\\s*\\b([a-zA-Z_$][\\w$]*)\\b\\s*:",
                            "beginCaptures": {
                                "1": {
                                    "name": "variable.parameter.function.luau"
                                }
                            },
                            "end": ",|(?=\\})",
                            "patterns": [
                                {
                                    "include": "#type_annotations"
                                }
                            ]
                        },
                        {
                            "begin": "\\s*\\b(\\[number\\])\\b\\s*:",
                            "beginCaptures": {
                                "1": {
                                    "name": "support.type.primitive.luau"
                                }
                            },
                            "end": ",|(?=\\})",
                            "patterns": [
                                {
                                    "include": "#type_annotations"
                                }
                            ]
                        },
                        {
                            "include": "#type_annotations"
                        }
                    ]
                },
                // Function types
                {
                    "begin": "\\s*\\(",
                    "beginCaptures": {
                        "0": {
                            "name": "punctuation.bracket.round.begin.luau"
                        }
                    },
                    "end": "\\)",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.bracket.round.end.luau"
                        }
                    },
                    "patterns": [
                        {
                            "begin": "\\s*\\b([a-zA-Z_$][\\w$]*)\\b\\s*:",
                            "beginCaptures": {
                                "1": {
                                    "name": "variable.parameter.function.luau"
                                }
                            },
                            "end": ",|(?=\\))",
                            "patterns": [
                                {
                                    "include": "#type_annotations"
                                }
                            ]
                        },
                        {
                            "include": "#type_annotations"
                        }
                    ]
                },
                // Function arrows
                {
                    "match": "\\->",
                    "name": "keyword.operator.arrow.luau"
                },
                // Type combination operators
                {
                    "match": "(?m)\\&|\\|",
                    "name": "keyword.operator.type.luau"
                },
                // String literal types
                {
                    "include": "#singleQuotedStringLiterals"
                },
                {
                    "include": "#doubleQuotedStringLiterals"
                },
                // True/False types
                {
                    "match": "\\s*\\b(true|false)\\b\\s*",
                    "name": "support.type.primitive.luau"
                },
                // Variable types/Generic types
                {
                    "begin": "(?m)\\s*(?=\\b([a-zA-Z_][\\w]*)\\b)",
                    "end": "(?m)(?<=(>|\\w))(?!\\s*(\\&|<|\\|))",
                    "name": "meta.type.annotation",
                    "patterns": [
                        {
                            "match": "\\&|\\|",
                            "name": "keyword.operator.type.luau"
                        },
                        {
                            "match": "\\b(any|nil|boolean|number|thread|string|unknown|never)\\b",
                            "name": "support.type.primitive.luau"
                        },
                        {
                            "match": "(?m)\\b([a-zA-Z_][\\w]*)\\b\\s*\\.",
                            "captures": {
                                "1": {
                                    "name": "entity.name.class.luau"
                                }
                            }
                        },
                        {
                            "match": "\\b([a-zA-Z_][\\w]*)\\b",
                            "captures": {
                                "1": {
                                    "name": "entity.name.type.luau"
                                }
                            }
                        },
                        {
                            "begin": "(?m)<",
                            "beginCaptures": {
                                "0": {
                                    "name": "punctuation.definition.arguments.begin.bracket.angle.luau"
                                }
                            },
                            "end": "(?m)>",
                            "endCaptures": {
                                "0": {
                                    "name": "punctuation.definition.arguments.end.bracket.angle.luau"
                                }
                            },
                            "patterns": [
                                {
                                    "include": "#type_annotations"
                                },
                                {
                                    "match": ",",
                                    "name": "punctuation.separator.comma.luau"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        "while_loops": {
            "begin": "(?=\\bwhile\\b)",
            "end": "(?<=\\bend\\b)",
            "patterns": [
                {
                    "begin": "\\bwhile\\b",
                    "beginCaptures": {
                        "0": {
                            "name": "keyword.control.while.luau"
